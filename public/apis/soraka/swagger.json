            "type": "string"
          },
          {
            "in": "query",
            "name": "is_current",
            "description": "是否为当前日期(0、1即可)",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "remark",
            "description": "备注",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "返回创建完成后的学期信息",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/termItem"
              }
            }
          },
          "500": {
            "description": "内部出错"
          }
        }
      }
    },
