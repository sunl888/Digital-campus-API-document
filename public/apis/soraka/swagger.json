        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "返回所有的学院/专业集合",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/collegeItem"
              }
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      }
    },
    "/excel/export/{tableName}": {
      "get": {
        "tags": [
          "excel"
        ],
        "summary": "导出指定的表",
        "description": "导出指定的表",
        "produces": [
          "application/vnd.ms-excel"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "tableName",
            "required": true,
            "description": "表名",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "导出指定的表到excel文件",
            "schema": {
              "type": "file"
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      }
    },
    "/excel/import/{tableName}": {
      "post": {
        "tags": [
          "excel"
        ],
        "summary": "导入指定的表",
        "consumes": [
          "multipart/form-data"
        ],
        "description": "导入指定的表",
        "produces": [
          "application/vnd.ms-excel"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "tableName",
            "required": true,
            "description": "表名",
            "type": "string"
          },
          {
            "in": "formData",
            "name": "file",
            "type": "file",
            "description": "The file to upload."
          }
        ],
        "responses": {
          "204": {
            "description": "导入指定的表到数据库",
            "schema": {
              "type": "file"
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      }
    },
    "/terms": {
      "get": {
        "tags": [
          "terms"
        ],
        "summary": "获取学期信息列表",
        "description": "获取学期信息列表",
