  "paths": {
    "/rooms": {
      "get": {
        "tags": [
          "rooms"
        ],
        "summary": "获取教室数据",
        "description": "获取教室数据",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "college_id",
            "description": "按学院筛选",
            "format": "integer32",
            "minimum": 0,
            "type": "integer"
          },
          {
            "in": "query",
            "name": "type_id",
            "description": "按类型筛选",
            "type": "integer",
            "format": "integer32",
            "minimum": 0
          },
          {
            "in": "query",
            "name": "only_trashed",
            "description": "查看已经删除的教室",
            "type": "boolean"
          }
        ],
        "responses": {
          "200": {
            "description": "返回筛选后的教室集合",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/roomItem"
              }
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      },
      "post": {
        "tags": [
          "rooms"
        ],
        "summary": "添加教室",
        "description": "添加教室",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "name",
            "required": true,
            "description": "教室名称",
            "type": "string"
          },
          {
            "in": "query",
            "name": "college_id",
            "required": true,
            "description": "专业ID",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "type_id",
            "required": true,
            "description": "教室类型ID",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "capacity",
            "required": true,
            "description": "教室容量",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "remark",
            "description": "备注",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "返回创建成功的教室",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/roomItem"
              }
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      }
    },
    "/rooms/{room_Id}": {
      "put": {
        "tags": [
          "rooms"
        ],
        "summary": "修改教室",
        "description": "修改教室",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "room_Id",
            "required": true,
            "description": "教室ID",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "name",
            "description": "教室名称",
            "type": "string"
          },
          {
            "in": "query",
            "name": "college_id",
            "description": "专业ID",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "type_id",
            "description": "教室类型ID",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "capacity",
            "description": "教室容量",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "remark",
            "description": "备注",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "返回更新成功的教室",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/roomItem"
              }
            }
          },
          "500": {
            "description": "内部错误"
          }
        }
      },
      "delete": {
        "tags": [
          "rooms"
        ],
        "summary": "删除教室",
        "description": "删除教室",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "room_Id",
            "required": true,
            "description": "教室ID",
            "type": "integer"
          }
        ],
        "responses": {
          "204": {
            "description": "返回 204 更新成功"
          },
          "500": {
            "description": "内部错误"
          }
        }
      }
    },
    "/room_types": {
      "get": {
        "tags": [
          "room_types"
        ],
        "summary": "获取教室类型",
        "description": "获取教室类型",
