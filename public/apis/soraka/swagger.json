          },
          "500": {
            "description": "内部出错"
          }
        }
      }
    }
  },
  "definitions": {
    "roomItem": {
      "type": "object",
      "required": [
        "id",
        "name",
        "capacity",
        "college",
        "college_str",
        "type",
        "remark",
        "created_at",
        "updated_at",
        "deleted_at"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "integer32",
          "example": 1
        },
        "name": {
          "type": "string",
          "example": "A101"
        },
        "capacity": {
          "type": "integer",
          "example": 120
        },
        "remark": {
          "type": "string",
          "example": null
        },
        "college": {
          "$ref": "#/definitions/collegeItem"
        },
        "college_str": {
          "type": "string",
          "description": "学院/专业转成字符串的形式便于显示",
          "example": "计算机学院计算机科学与技术专业"
        },
        "type": {
          "$ref": "#/definitions/typeItem"
        },
        "created_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "updated_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "deleted_at": {
          "type": "string",
          "example": null
        }
      }
    },
    "typeItem": {
      "required": [
        "id",
        "name",
        "parent_id",
        "created_at",
        "updated_at",
        "deleted_at"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "example": 1
        },
        "name": {
          "type": "string",
          "example": "多媒体教室"
        },
        "parent_id": {
          "type": "integer",
          "example": 0
        },
        "created_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "updated_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "deleted_at": {
          "type": "string",
          "example": null
        }
      }
    },
    "collegeItem": {
      "required": [
        "id",
        "name",
        "parent_id",
        "created_at",
        "updated_at",
        "deleted_at"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "example": 2
        },
        "name": {
          "type": "string",
          "example": "计算机学院"
        },
        "parent_id": {
          "type": "integer",
          "example": 1
        },
        "created_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "updated_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "deleted_at": {
          "type": "string",
          "example": null
        }
      }
    },
    "roomTypeItem": {
      "required": [
        "id",
        "name",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "example": 2
        },
        "name": {
          "type": "string",
          "example": "普通教室|多媒体教室|..."
        },
        "parent_id": {
          "type": "integer",
          "example": 1
        },
        "created_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        },
        "updated_at": {
          "type": "string",
          "example": "2018-04-18 13:46:11"
        }
      }
    },
    "termItem": {
      "required": [
        "id",
        "creator_id",
        "name",
        "semester",
        "date_of_school",
        "holiday",
        "is_current",
        "remark",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "example": 1
        },
        "creator_id": {
          "type": "integer",
          "example": 1
        },
        "name": {
          "type": "string",
          "example": "2017~2018"
        },
        "semester": {
          "type": "integer",
          "example": "1|2"
        },
        "date_of_school": {
          "type": "string",
          "example": "2018-04-23T00:00:00.000Z"
        },
        "holiday": {
          "type": "string",
          "example": "2018-04-23T00:00:00.000Z"
        },
        "is_current": {
          "type": "boolean",
          "example": 1
        },
        "remark": {
          "type": "string",
          "example": "我是备注"
        },
        "created_at": {
          "type": "string",
          "example": "2018-04-18 18:09:20"
        },
        "updated_at": {
          "type": "string",
          "example": "2018-04-18 18:09:20"
        }
      }
    }
